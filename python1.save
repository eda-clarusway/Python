rows = 5
for i in range(0,rows):
   for j in range(0,i+1):
     print("*", end = " ")
   print("\n")
for i in range(rows, 0 , -1):
    for j in range(0, i-1):
        print("*", end = " ")
    print("\n")



for number in range(3,100):
  counter = 0
  for i in range(2,int(number/2)+1):
      if number % i == 0:
        counter += 1
        break
  if counter == 0:
        print(number,end = " ")
      
